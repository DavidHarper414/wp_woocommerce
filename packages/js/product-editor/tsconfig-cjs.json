{
	"extends": "../tsconfig-cjs",
	"compilerOptions": {
		"outDir": "build",
		"noCheck": false,
		"resolveJsonModule": true,
		"typeRoots": [
			"./typings",
			"./node_modules/@types"
		]
	},
	"include": [
		"typings/**/*",
		"src/**/*",
	],
	"exclude": [
		"./wireit",
		"./build",
		"./node_modules",
		"./build-module",
		"./build-style",
		"./build-types"
	]
}
