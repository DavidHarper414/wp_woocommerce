{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "woocommerce/product-input-field",
	"title": "Product input control",
	"category": "woocommerce",
	"description": "A input control for use in the product editor.",
	"keywords": [ "products", "input" ],
	"textdomain": "default",
	"attributes": {
		"label": {
			"type": "string",
			"__experimentalRole": "content"
		},
		"property": {
			"type": "string"
		},
		"placeholder": {
			"type": "string"
		},
		"required": {
			"type": "boolean"
		},
		"validationRegex": {
			"type": "string"
		},
		"validationErrorMessage": {
			"type": "string"
		}
	},
	"supports": {
		"align": false,
		"html": false,
		"multiple": true,
		"reusable": false,
		"inserter": false,
		"lock": false,
		"__experimentalToolbar": false
	},
	"editorStyle": "file:./editor.css"
}
