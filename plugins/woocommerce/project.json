{
  "root": "plugins/woocommerce/",
  "sourceRoot": "plugins/woocommerce",
  "projectType": "application",
  "implicitDependencies": [ "woocommerce-legacy-assets" ],
  "targets": {
    "composer-install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "composer install",
        "cwd": "plugins/woocommerce"
      }
    },
    "composer-install-no-dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "composer install --no-dev",
        "cwd": "plugins/woocommerce"
      }
    },
    "composer-dump-autoload": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "composer dump-autoload",
        "cwd": "plugins/woocommerce"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "./bin/build-zip.sh",
        "cwd": "plugins/woocommerce"
      }
    },
    "build-watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "grunt watch",
        "cwd": "plugins/woocommerce/legacy"
      }
    },
    "build-assets": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpm nx build woocommerce-legacy-assets",
        "cwd": "plugins/woocommerce"
      }
    },
    "lint-js": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "eslint legacy/js --ext=js",
        "cwd": "plugins/woocommerce"
      }
    },
    "docker-up": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-e2e docker:up",
        "cwd": "plugins/woocommerce"
      }
    },
    "docker-down": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-e2e docker:down",
        "cwd": "plugins/woocommerce"
      }
    },
    "docker-ssh": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-e2e docker:ssh",
        "cwd": "plugins/woocommerce"
      }
    },
    "test-api": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-api-tests test api",
        "cwd": "plugins/woocommerce"
      }
    },
    "test-e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-e2e test:e2e",
        "cwd": "plugins/woocommerce"
      }
    },
    "test-e2e-debug": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-e2e test:e2e-debug",
        "cwd": "plugins/woocommerce"
      }
    },
    "test-e2e-dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pnpx wc-e2e test:e2e-dev",
        "cwd": "plugins/woocommerce"
      }
    },
    "test-unit": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "./vendor/bin/phpunit -c ./phpunit.xml",
        "cwd": "plugins/woocommerce"
      }
    },
    "packages-fix-text-domain": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "packages:fix:textdomain"
      }
    },
    "git-update-hooks": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "git:update-hooks"
      }
    },
    "make-collection": {
      "executor": "@nrwl/workspace:run-script",
      "options": {
        "script": "make:collection"
      }
    },
    "install-unit-test-db": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "bash tests/bin/install.sh woo_test root root 127.0.0.1 latest",
        "cwd": "plugins/woocommerce"
      }
    },
    "test-code-coverage": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "RUN_CODE_COVERAGE=1 bash tests/bin/phpunit.sh",
        "cwd": "plugins/woocommerce"
      }
    }
  }
}
